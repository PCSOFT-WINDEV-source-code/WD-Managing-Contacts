     name : ResizeDialog
     procedure_id : 1751431051372287662
     type_code : 14
     code : |1+
      Procedure ResizeDialog()
      // Update the size of the window according to the caption
      
      // min width: 
      // the one of buttons + a margin on the sides
      nMinWidth is int = nBtnWidth +  MARGINBORDERS
      // max width of the caption
      nMaxCaptionWidth is int = 600
      
      
      // dimensions required to draw the caption
      //@@+10: Additional margin
      nTextWidth is int = TextWidth(STA_INFO, szCaption) + 10
      // the caption is not allowed if it is too wide
      IF nTextWidth > nMaxCaptionWidth THEN
      	nTextWidth = nMaxCaptionWidth	
      END
      // force the width of the caption in order for the calculation of the height to be correct
      STA_INFO..Width = nTextWidth
      nTextHeight is int = TextHeight(STA_INFO, szCaption, tMultiline)
      // height not less than the height of the icon on the left
      IF nTextHeight< ICON_INFORMATION..Height THEN nTextHeight =  ICON_INFORMATION..Height
      // center the caption in height
      STA_INFO..VerticalAlignment = vaMiddle
      
      // Width of the window
      nWinWidth is int = nTextWidth +  MyWindow..Width - STA_INFO..InitialWidth
      // the width must not be less than MIN width.
      IF nWinWidth < nMinWidth THEN 
      	nWinWidth = nMinWidth
      END
      
      
      // Height of the window
      nWinHeight is int = MyWindow..Height + nTextHeight - STA_INFO..InitialHeight
      
      // center the window in width and in height
      MyWindow..X -= ( nWinWidth - MyWindow..Width  )/2
      MyWindow..Y -= ( nWinHeight - MyWindow..Height  )/2
      
      // Update the size of the window
      WinSize("", nWinWidth, nWinHeight)
      
      // Update the size of the caption
      STA_INFO..Width = nTextWidth
      STA_INFO..Height = nTextHeight
      STA_INFO = szCaption
      
     type : 458752
   -
     name : CenterTheButtons
     procedure_id : 1751431051372353198
     type_code : 14
     code : |1+
      Procedure CenterTheButtons()
      // center the dialog buttons
      
      // start position
      xStart is int =  (WinInWidth() - nBtnWidth) / 2
      
      // For Info/Error/Warning
      BTN_OK..X = xStart
      
      // For YesNo/OKCancel
      BTN_YES..X = xStart
      BTN_NO..X = xStart + BTN_YES..Width + MARGINBETWEENBTN
      
      // For Confirm
      BTN_CANCEL..X = BTN_NO..X + BTN_NO..Width + MARGINBETWEENBTN
      
      
      // center the buttons in height
      // (they are all stacked but a single one is visible)
      ICON_INFORMATION..Y = STA_INFO..Y + (STA_INFO..Height - ICON_INFORMATION..Height) / 2
      ICON_QUESTION..Y = ICON_INFORMATION..Y 
      ICON_ERROR..Y =   ICON_INFORMATION..Y 
      
     type : 458752
   -
     name : WarningSound
     procedure_id : 1751431051372418734
     type_code : 14
     code : |1+
      Procedure WarningSound()
      // Emits a beep like the system
      
      SWITCH nType
      	CASE 0 : // Info
      		CallDLL32("USER32","MessageBeep", MB_ICONASTERISK )
      	CASE 1 : // Error
      		CallDLL32("USER32","MessageBeep", MB_ICONHAND )
      	CASE 2 : // YesNo
      		CallDLL32("USER32","MessageBeep", MB_ICONQUESTION )
      	CASE 3 : // Confirm
      		CallDLL32("USER32","MessageBeep", MB_ICONQUESTION )
      	CASE 4 : // Warning
      		CallDLL32("USER32","MessageBeep", MB_ICONASTERISK )
      	CASE 5 : // OKCancel
      		CallDLL32("USER32","MessageBeep", MB_ICONQUESTION )				
      END
      
