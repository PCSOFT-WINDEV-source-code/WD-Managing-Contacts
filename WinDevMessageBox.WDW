      Procedure WinDevMessageBox( szTitle="test", szCaption="Test Message", nType=1, nButtonNum=1)
      // Alternative window to the standard MessageBox of Windows.
      // This window is added to the project if 
      // "[] apply the theme of skin template to the system windows" is checked.
      // It is used by the following functions:
      // -Info()
      // -Error()
      // -YesNo()
      // -Confirm()
      //  -Warning()
      // -OKCancel()
      // Parameters:
      //  szTitle:  Title that will be given to the window
      //  szCaption: Message that will be displayed in the window
      //  nType: Dialog to emulate: 0 = Info, 1 = Error, 2 = YesNo, 3 = Confirm, 4 = Warning, 5 = OKCancel.
      //  nButtonNumber:  number of the button to select (for YesNo and Confirm)
      //
      // Last code review: PAD 03/24/2004
      
      GLOBAL
      	// Width for the dialog buttons
      	// (Calculated according to the type of dialog)
      	nBtnWidth is int = 0
      // margin in addition to the buttons for MinWidth
      CONSTANT MARGINBORDERS  = 20
      // margin between the buttons
      CONSTANT MARGINBETWEENBTN  = 6
      
      // constants for MessageBeep
      CONSTANT MB_OK              = 0x00000000
      CONSTANT MB_ICONHAND     	= 0x00000010
      CONSTANT MB_ICONQUESTION    = 0x00000020
      CONSTANT MB_ICONEXCLAMATION = 0x00000030
      CONSTANT MB_ICONASTERISK    = 0x00000040
      
      // constants returned by MessageBox
      CONSTANT IDOK     =           1
      CONSTANT IDCANCEL =  	      2
      CONSTANT IDABORT  =           3
      CONSTANT IDRETRY  =           4
      CONSTANT IDIGNORE =           5
      CONSTANT IDYES    =           6
      CONSTANT IDNO     =           7
      
      
      
      // implement the message
      STA_INFO = szCaption
      // implement the title
      MySelf..Title = szTitle
      
      // implement the buttons
      SWITCH nType
      	CASE 0 : // Info
      		// make the [OK] button and the "I" icon visible
      		BTN_OK..Visible = True	
      		ICON_INFORMATION..Visible = True	
      		// the width is the one of a button
      		nBtnWidth = BTN_OK..Width 
      		// Value returned when the cross is clicked: OK
      		MySelf..ReturnedValue = IDOK
      	CASE 1 : // Error
      		// make the [OK] button and the "x" icon visible
      		BTN_OK..Visible = True
      		ICON_ERROR..Visible = True
      		// the width is the one of a button
      		nBtnWidth =  BTN_OK..Width 
      		// Value returned when the cross is clicked: OK
      		MySelf..ReturnedValue = IDOK		
      	CASE 2 : // YesNo
      		// make the [YES] and [NO] buttons and the "?" icon visible
      		BTN_YES..Visible = True		
      		BTN_NO..Visible = True		
      		ICON_QUESTION..Visible = True	
      		// the width is the one of 2  buttons + the margin 
      		nBtnWidth = BTN_YES..Width *2 + MARGINBETWEENBTN
      		// to select [NO] as default button
      		IF nButtonNum=2 THEN ScreenFirst(BTN_NO)
      		// Value returned when the cross is clicked: NO
      		MySelf..ReturnedValue = IDNO			
      	CASE 3 : // Confirm
      		// make the [YES], [NO] and [CANCEL] buttons and the "?" icon visible
      		BTN_YES..Visible = True		
      		BTN_NO..Visible = True		
      		BTN_CANCEL..Visible = True	
      		ICON_QUESTION..Visible = True	
      		// the width is the one of 3 buttons + the margin 
      		nBtnWidth =  BTN_YES..Width *3 + MARGINBETWEENBTN*2
      		// to select [NO] as default button
      		IF nButtonNum=2 THEN ScreenFirst(BTN_NO)
      		// to select [Cancel] as default button
      		IF nButtonNum=3 THEN ScreenFirst(BTN_CANCEL)
      		// Value returned when the cross is clicked: CANCEL
      		MySelf..ReturnedValue = IDCANCEL	
      	CASE 4 : // Warning
      		// make the [OK] button and the "!" icon visible
      		BTN_OK..Visible = True	
      		ICON_INFORMATION..Visible = True	
      		// the width is the one of a button
      		nBtnWidth = BTN_OK..Width 
      		// Value returned when the cross is clicked: OK
      		MySelf..ReturnedValue = IDOK	
      	CASE 5 : // YesNo
      		// make the [YES] and [NO] buttons and the "I" icon visible
      		BTN_YES..Visible = True		
      		BTN_NO..Visible = True		
      		ICON_QUESTION..Visible = True	
      		// Assign "OK" to [YES] and "Cancel" to [NO]
      		BTN_YES..Caption = BTN_OK..Caption
      		BTN_NO..Caption = BTN_CANCEL..Caption
      		// the width is the one of 2  buttons + the margin 
      		nBtnWidth = BTN_YES..Width *2 + MARGINBETWEENBTN
      		// to select [Cancel] as default button
      		IF nButtonNum=2 THEN ScreenFirst(BTN_NO)
      		// Value returned when the cross is clicked: NO
      		MySelf..ReturnedValue = IDNO								
      END
      
      // calculates the width and height of the dialog box
      ResizeDialog()
      
      // centers the buttons 
      CenterTheButtons()
      
      // emit a beep (like the system)
      WarningSound()
      
      
