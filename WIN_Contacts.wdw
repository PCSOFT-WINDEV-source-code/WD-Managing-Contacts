        internal_properties : 
        code : |1+
         
         //Automatic process of the management of errors defined in the bar of the code editor:
         //if error: display the error and resume the input in the current window
         //No code is required to manage the errors in this process
         
         
         <SI CibleExécution=ApplicationWindows>
         	
         	//1: &Import
         	//2: &Do not import
         	SWITCH Dialog(<§?184961A4043645C4§>, SPC)
         	
         		// &Import
         		CASE 1 : 
         			Info("The Outlook contacts with no name and no company will not be added to the application.")
         			
         			//Synchronize the contacts
         			SynchronizeOutlookContacts()
         			
         			//We want to see all the contacts resulting from the synchronization			//=> reinitialize the quick search control
         			EDT_Find = "" 
         			//and reinitialize the display of the looper 
         			LooperDisplay(LOOP_Contacts, taInit)
         			
         		// &Do not import
         		CASE 2 :
         			
         	END
         	
         <FIN>
         
