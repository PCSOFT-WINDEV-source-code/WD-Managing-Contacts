        code : |1+
         // Checks whether the looper is empty or not
         IF LOOP_Contacts..Occurrence = 0 THEN RETURN
         
         //Retrieve the IDs of all the contacts selected in the looper
         sListIDContacts is string  
         sContactID is string
         FOR ALL SELECTED ROW of LOOP_Contacts
         	sContactID = LOOP_Contacts.ATT_CONTACTID
         	sListIDContacts = sListIDContacts + [TAB] + sContactID
         END
         
         //Open the window for sending emails and pass in parameter the IDs of the contacts selected in the looper
         Open(WIN_SendEmail, sListIDContacts)
         
