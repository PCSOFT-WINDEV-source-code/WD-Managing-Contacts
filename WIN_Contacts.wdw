      -
        internal_properties : CAAAAAgAAAA41SPhZ/4jG7Ga+OKD88bdXuNi/8hbl4LGMRNwYpjGoeYwr01p60KA74Ex5+YALpvZUlizsLNY9ijNl9sn/jgThpwEZb09Yxum5sWn5HXigUeDUXJBXTkW9M6tllBheuXdHnqOEZ3PPKyzXRUY39nwfcav8BZyJYB9W3praePXzp2Y6U0yrnaX2g+AP2Zno+18
        code : |1-
         // Checks whether the looper is empty or not
         IF LOOP_Contacts..Occurrence = 0 THEN RETURN
         
         //A single contact can be modified at a time 
         nSubscriptSelection1 is int
         
         //If several contacts are selected
         IF LooperSelectCount( LOOP_Contacts) > 1 THEN
         	//Retrieve the 1st selected one 
         	 nSubscriptSelection1 = LooperSelect(LOOP_Contacts,1)
         	//Deselect all
         	LooperSelectMinus(LOOP_Contacts)
         	
         	//and reselect the 1st one only
         	LooperSelectPlus(LOOP_Contacts, nSubscriptSelection1)
         END
         
         //Open the "Contact Form" window and pass the identifier of the contact to modify in parameter
         Open(WIN_ContactForm, LOOP_Contacts.ATT_CONTACTID)
         
         //Then, redisplay the looper to view the modifications made to the contact
         LooperDisplay(LOOP_Contacts, taCurrentRecord)
