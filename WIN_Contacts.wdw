      -
        internal_properties : CAAAAAgAAADKM/B8PidiEjx1AhlIpRUcBkxv5Ua7XsZF7iXQxeUAX8FHLDyEL/PX+kkE3LQWgdPlvuNnqNNXCoDoekFSAhptUZukuIo5vZAoGWjuPluO6J9x4bvyww5SMZ+juR5a94wqi8yDKeX0GKe+wbrIpo0ubcUHuZ6Kmpcha7UPprENlERfk9bqFuGpLLxKAFfI5eIRSWZ9YRDG4zMqgS7js78RlpAGKhxCZWUYEA==
        code : |1-
         // Checks whether the looper is empty or not
         IF LOOP_Contacts..Occurrence = 0 THEN RETURN
         
         //Several contacts can be selected
         //=> browse all the selected contacts and ask the user to confirm the deletion for each one of them
         FOR ALL SELECTED ROW of LOOP_Contacts
         	
         	//1: &Delete
         	//2: Do&n't delete
         	SWITCH Dialog(<§?18495EC201DA01E2§>, LOOP_Contacts.ATT_LASTNAME + " " + LOOP_Contacts.ATT_FIRSTNAME )
         	
         		// &Delete
         		CASE 1 :
         			//Position in the file on the contact to delete: the one selected in the looper
         			HReadSeekFirst(Contact,ContactID,LOOP_Contacts.ATT_CONTACTID)
         			//Delete the selected contact
         			HDelete(Contact)
         					
         		// Do&n't delete
         		CASE 2 :
         			
         	END
         		
         END
         
         //Redisplay the looper without the deleted contacts
         LooperDisplay(LOOP_Contacts,taInit)
