      // Summary: Initialize the attachments
      // Syntax:
      // InitATT ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      Procedure InitATT()
      
      // Number of attachments
      Email.NbAttach = TableCount(TABLE_AttachedFiles)
      
      // Initialize the array of attachments
      FOR ALL ROW n of TABLE_AttachedFiles
      	Email.Attach[n] = TABLE_AttachedFiles.FilePath[n]
      END
