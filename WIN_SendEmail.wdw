        code : |1-
         bModifiedOptions is boolean = False
         
         //If we were located on the tab of options and if these options have been modified by the user;
         //propose to store these new options
         IF gnPreviousTab = 3 THEN
         	
         
         <SI CibleExécution=ApplicationWindows>
         		
         		// The options have been modified if one of the group controls was modified
         		bModifiedOptions = GR_EmailOption..Modified
         	
         <SINON SI CibleExécution=ApplicationLinux32>
         	
         		// The options have been modified if one of the group controls was modified
         		bModifiedOptions = (RADIO_Confidentiality..Modified OR RADIO_Priority..Modified OR CBOX_Receipt..Modified OR CBOX_DispositionNotification..Modified)
         	
         <FIN>
         
         
         
         		IF bModifiedOptions THEN
         		//The WLanguage function named "Dialog" enables you to propose clear error messages and clear answers to the users
         		//This function includes a wizard that helps you
         		
         		//1: &Save
         		//2: &Do not save
         		SWITCH Dialog(<§?184D35840097ED77§>)
         		
         			// Sav&e
         			CASE 1 :
         				//Store the options
         				SaveParameter(CST_ACKNOWLEDGMENT_RECEIPT, CBOX_Receipt)
         				SaveParameter(CST_READING_CONFIRMATION, CBOX_DispositionNotification)
         				SaveParameter(CST_CONFIDENTIALITY, RADIO_Confidentiality)
         				SaveParameter(CST_PRIORITY, RADIO_Priority)
         				
         			// &Don't save
         			CASE 2 :
         				
         		END
         		
         		GR_EmailOption..Modified = False
         		
         	END
         END
         
         // Changes the window plane according to the tab pane
         MyWindow..Plane = MySelf
         
         // Update the number of attachments
         IF TAB_Details = 1 THEN
         	STC_NbAttachedFiles = "%1 attached files have been found"
         	STC_NbAttachedFiles = StringBuild(STC_NbAttachedFiles,TableCount(TABLE_AttachedFiles))
         END
         
         gnPreviousTab = MySelf
