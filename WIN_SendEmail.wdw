      // Summary: Initializes the options for sending emails according to the options checked by the user
      // Syntax:
      // InitEmailOptions ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      Procedure InitEmailOptions()
      
      // Initialize the option variables
      Email.ReturnReceipt = CBOX_Receipt
      Email.DispositionNotification = CBOX_DispositionNotification
      
      SWITCH RADIO_Confidentiality
      	CASE 1 :// Normal
      		Email.Sensitivity = emailNoSensitivity
      	CASE 2 :// Confidential
      		Email.Sensitivity = emailCompanyConfidential
      	CASE 3 :// Personal
      		Email.Sensitivity = emailPersonal
      	CASE 4 :// Private
      		Email.Sensitivity = emailPrivate
      END
      
      SWITCH RADIO_Priority
      	CASE 1 : // high
      		Email.Priority = emailHighPriority
      	CASE 2 : // Standard
      		Email.Priority = emailNormalPriority
      	CASE 3 : // Low
      		Email.Priority = emailLowPriority
      END
