     name : InitRecipients
     internal_properties : CAAAAAgAAAASvddaaHAZHEYGMSEMVBPslwJnGsAie3EwHq2J2SYImsTcdNjMpu4VXE08UfJxIxXHeCC6pErp5PZvzJYWRPJOjUQNUfok7hpD8Ep1oeK3fTw+p1o1yXzvToHnPF89z4qy9acKNHQks7S1/XVaBTpLTrDfsy52RYbsJ7uuzeL+G/xCEZLcCD+GTbCvbIXyLCAH5FW5w+ZxN0onZ1vw6gI8DASfaA==
     procedure_id : 1354898758453552539
     type_code : 14
     code : |1+
      // Summary: Initialize the recipients for sending emails
      // Syntax:
      //InitRecipients (<sListRecipients>, <nRecipType>)
      //
      // Parameters:
      //	sListRecipients: String containing the list of recipients separated by ";"
      //	nDestType: 1 for the standard recipients, 2 for CC, 3 for BCC
      // Return value:
      // 	None
      
      Procedure InitRecipients(sListRecipients, nRecipType)
      
      sRecipient is string
      nNbRecipient, i are int
      parrRecipient is array dynamic
      
      // Retrieve the numbers of recipients
      nNbRecipient = StringCount(sListRecipients,";")
      
      //Assign the number of recipients
      SWITCH nRecipType 
      	CASE 1 :
      		// Main recipients
      		Email.NbRecipient = nNbRecipient
      	CASE 2 :
      		// CC recipients
      		Email.NbCc = nNbRecipient
      	CASE 3 :
      		// CCi recipients
      		Email.NbBcc = nNbRecipient
      END
      
      // Allocate the array according to the number of recipients
      parrRecipient = new array dynamic of nNbRecipient strings
      
      // Fill the array of recipients
      i = 1
      FOR EACH string sRecipient of sListRecipients SEPAREE by ";"
      	IF sRecipient <> "" THEN
      		parrRecipient[i] = sRecipient
      		i++
      	END
      END
      
      //No need to declare the j.. variable, it is done by the WLanguage 
      FOR j = 1 TO nNbRecipient
      	// Initialize the recipient variable, cc or bcc
      	SWITCH nRecipType
      		CASE 1 :		
      			// Main recipients
      			Email.Recipient[j] = parrRecipient[j]
      		CASE 2 :
      			// CC recipients
      			Email.Cc[j] = parrRecipient[j]
      		CASE 3 :
      			// CCi recipients
      			Email.Bcc[j] = parrRecipient[j]
      	END
      END
      
      // Free the dynamic array
      delete parrRecipient
