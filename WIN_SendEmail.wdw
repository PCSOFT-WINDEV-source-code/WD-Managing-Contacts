    name : BTN_Send
    identifier : 0x22cd913e20f2e59b
    internal_properties : 
    properties :
     x : 548
     y : 572
     width : 118
     height : 34
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : 
        code : |1+
         
         //Check whether a recipient was specified for the email
         IF EDT_Recipients = "" THEN
         	
         	Error("Enter a recipient.")
         	ReturnToCapture(EDT_Recipients)
         	
         ELSE
         	
         	HourGlass(True)
         	
         	//Reset all the members of the Email structure to 0
         	EmailReset()
         	
         	//Initialize the email options: confirmation of reading, priority...
         	InitEmailOptions()
         	
         	//Main information about the email...
         	Email.Sender = gsEmailSender
         	Email.Subject = EDT_SUBJECT
         	//The WLanguage function named "RTFToHTML" is used to transform RTF into HTML
         	Email.HTML = EDHTML_email
         	
         	
         	//Initialize the different types of recipients
         	InitRecipients(EDT_Recipients,1)
         	InitRecipients(EDT_CC,2)
         	InitRecipients(EDT_BCC,3)
         	
         	//Initialize the attachments
         	InitATT()
         	
         	//Open the SMTP connection
         	IF NOT EmailStartSMTPSession(gsUserName,gsPassWord,gsSMTPServer) THEN
         		
         		Error("Unable to establish the connection to the server.","Retry later or check your parameters.")
         		
         	ELSE
         		
         
         <SI CibleExécution=ApplicationWindows>
         	
         		// Opens the progress window
         		OpenChild(WIN_Progbar)
         		//Progress bar used to view the progress of the sending
         		EmailProgressBar(WIN_Progbar.PROGBAR_SendEmail)
         	
         <FIN>
         
         	
         		//Post the message
         		IF NOT EmailSendMessage(gsUserName) THEN
         			HourGlass(False)
         			Error("This email cannot be sent. Check the setting of the email account.")
         			ReturnToCapture(MyWindow)
         		ELSE
         			HourGlass(False)
         			Info("The email was successfully sent.")
         		END
         		
         
         <SI CibleExécution=ApplicationWindows>
         	
         			//Stop the progress bar
         			EmailProgressBar("")
         	
         <FIN>
         
         
         		// Close the SMTP session
         		EmailCloseSession(gsUserName)
         		
         	END
         	
         	HourGlass(False)
         	Close()
         	
         END
         
