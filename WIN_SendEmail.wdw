      Procedure WIN_SendEmail( sListIDContacts )
      
      //Declaration of the variables global to the window
      gsEmailSender, gsUserName, gsPassWord, gsSMTPServer, gsEmailAddressContact are strings
      gnPreviousTab is int
      
      sContactID is string
      FOR EACH string sContactID of sListIDContacts
      	
      	//Position in the file on the contact whose identifier if sContactID
      	HReadSeekFirst(Contact, ContactID, Val(sContactID))
      	//Check whether the contact has an email address
      	IF Contact.Email <> "" THEN
      		//if so, add it to the list of recipient email addresses
      		gsEmailAddressContact = gsEmailAddressContact + [";"] + Contact.Email
      	END
      	
      END
   -
     internal_properties : CAAAAAgAAABc35IMEUjPOsweLXRRQTjZu1/FIX98QBs0gj5N6nMuKaTfS/ipvWlc6wsHhFXewxybZWmx+qIKAD3fTNqUhT09XGxzNqMuT3voI8V3jJLQSDEIwp5jhsTubyW245AFjIZXB6ZN8jA/SGh+d/cKUVJ7wKEVMwrlLtF9+a42yWER6lpxj+KaKjobYj4Pa8b2VgZ4FhmOXK1QxVwrL+v63vo+bTrvlY8Go9N9CuVgFnyohOuUgU1jZM15cqo=
     code : |1+
      // Automatic process of errors: display the error and resume the input
      //
      
      //By default, position on the 1st plane of the window
      MyWindow..Plane = 1
      
      //Position on the 1st tab by default
      TAB_Details = 1
      
      IF fFileExist(gsConfigFile) = False THEN  
      	
      	//Open the setting window to allow the user to specify this information	
      	Open(WIN_ConfigureEmail)	
      	
      END
      
       i is int = 1
       
      //Otherwise, retrieve the information stored in the configuration file
      LOOP
      	
      	//No indefinite loop on the setting window => after a while, cancel the sending of emails
      	IF i = 3 THEN 
      		Error("The information for configuring the email account was not entered properly. The window for sending emails will be closed.") 
      		Close()
      		BREAK
      	END
      	
      	// Retrieve the information for configuring the email account
      	gsEmailSender = LoadParameter(CST_STORE_EMAIL, "")
      	gsUserName = ExtractString(gsEmailSender,1,"@")
      	gsPassWord = Uncrypt(LoadParameter(CST_STORE_PWD, ""),"FD45SR44K")
      	gsSMTPServer = LoadParameter(CST_STORE_SMTP_SERVER, "")
      	
      	// Display the options stored in the window
      	CBOX_Receipt = LoadParameter(CST_ACKNOWLEDGMENT_RECEIPT, "")
      	CBOX_DispositionNotification = LoadParameter(CST_READING_CONFIRMATION, "")
      	RADIO_Confidentiality = LoadParameter(CST_CONFIDENTIALITY, "")
      	RADIO_Priority = LoadParameter(CST_PRIORITY, "")
      	
      	//If the information about the user name and the SMTP server to use is still missing
      	IF gsUserName = "" OR gsSMTPServer = "" THEN
      		//open the setting window to allow the user to specify this information,
      		//information required to start a session for sending emails by SMTP	
      		Open(WIN_ConfigureEmail)	
      	ELSE
      		//If the information was found, exit from the loop
      		BREAK
      	END
      	
      	i++
      	
      END
      
      //If we are located in this window, then we know the email address of the contact, 
      //=> automatically add it to the list of recipients
      //indeed, the "Email" button of the "WIN_Contacts" window is grayed if the contact has no email
      EDT_Recipients = gsEmailAddressContact +";"
